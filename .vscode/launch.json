    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) rgbd_tum",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Examples/RGB-D/rgbd_tum",
            "args": [
                "Vocabulary/ORBvoc.txt",
                "Examples/RGB-D/TUM3.yaml",
                "/home/yubao/data/Dataset/TUM/freiburg3/rgbd_dataset_freiburg3_walking_xyz",
                "/home/yubao/data/Dataset/TUM/freiburg3/rgbd_dataset_freiburg3_walking_xyz/associate.txt",
                "/home/yubao/data/Project/Mask_RCNN/",
                "/tmp"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}